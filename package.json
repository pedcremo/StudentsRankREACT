{
  "name": "babel",
  "version": "1.0.0",
  "description": "Proves de transpilacio amb babel",
  "main": "main.js",
  "scripts": {
    "test": "karma start --single-run --no-auto-watch karma.conf.js",
    "e2e-test": "(webdriver-manager start &) && sleep 2 && (babel-node src/server/appDev.js &) && protractor tests_e2e/conf.js",
    "prestart": "babel-node tools/startMessage.js",
    "start": "forever start src/server/app.js NODE_ENV='build'",
    "dev": "babel-node src/server/appDev.js",
    "stop": "forever stop src/server/app.js",
    "build:html": "babel-node tools/buildHTML.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "node src/server/app.js",
    "restart": "forever restart src/server/app.js",
    "doc": "jsdoc -c jsdoc.json README.md src/client/**/*",
    "make:traductions": "node ./src/server/scripts/traductor.js"
  },
  "author": "Pere Crespo",
  "license": "ISC",
