{
  "name": "babel",
  "version": "1.0.0",
  "description": "Proves de transpilacio amb babel",
  "main": "main.js",
  "scripts": {
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "prestart": "babel-node tools/startMessage.js",
    "start": "forever NODE_ENV='build' start src/server/app.js",
    "dev": "babel-node src/server/appDev.js",
    "stop": "forever stop src/server/app.js",
    "build:html": "babel-node tools/buildHTML.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "node src/server/app.js",
    "restart": "forever restart src/server/app.js"
  },
  "author": "Pere Crespo",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.8.2",
    "bootstrap": "4.0.0-beta.2",
    "child_process": "^1.0.2",
    "express": "^4.9.3",
    "express-force-ssl": "^0.3.2",
    "express-session": "*",
    "font-awesome": "^4.7.0",
    "forever": "^0.15.3",
    "formidable": "^1.1.1",
    "fs": "0.0.1-security",
    "github-webhook-handler": "^0.7.0",
    "install": "^0.10.2",
    "jquery": "^3.2.1",
    "lowdb": "^1.0.0",
    "morgan": "^1.1.1",
    "npm": "^5.6.0",
    "passport": "*",
    "passport-facebook": "~2.1.1",
    "passport-google-oauth": "1.0.0",
    "passport-google-oauth2": "^0.1.6",
    "passport-local": "*",
    "passport-twitter": "~1.0.4",
    "popper.js": "^1.12.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "serve-favicon": "^2.0.1",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
