    "child_process": "^1.0.2",
    "cors": "^2.8.4",
    "express": "^4.9.3",
    "express-force-ssl": "^0.3.2",
    "express-session": "*",
    "font-awesome": "^4.7.0",
    "forever": "^0.15.3",
    "formidable": "^1.1.1",
    "fs": "0.0.1-security",
    "github-webhook-handler": "^0.7.0",
    "gmail-node": "^1.2.0",
    "google-translate-api": "^2.3.0",
    "i18n-react": "^0.6.3",
    "install": "^0.10.2",
    "jquery": "^3.2.1",
    "lowdb": "^1.0.0",
    "morgan": "^1.1.1",
    "npm": "^6.1.0",
    "observable-map": "0.0.4",
    "passport": "*",
    "passport-facebook": "~2.1.1",
    "passport-google-oauth": "1.0.0",
    "passport-google-oauth2": "^0.1.6",
